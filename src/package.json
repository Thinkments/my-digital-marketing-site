{
"name": "thinkments-website",
"version": "1.0.0",
"description": "ThinkMents Digital Marketing & Web Design Agency Website - Pure Frontend Application",
"private": true,
"type": "module",
"deploymentType": "frontend-only",
"supabase": {
  "enabled": false
},
"scripts": {
"dev": "vite",
"build": "npm run generate-robots && npm run copy-static && tsc && vite build",
"preview": "vite preview",
"generate-robots": "node scripts/generate-robots.js",
"copy-static": "node scripts/copy-static-files.js",
"seo:generate": "node scripts/seo-improvements.js",
"seo:check": "node scripts/seo-check.js",
"build:seo": "npm run seo:generate && npm run build",
"deploy": "npm run build"
},
"dependencies": {
"react": "^18.2.0",
"react-dom": "^18.2.0",
"react-helmet-async": "^1.3.0",
"lucide-react": "^0.263.1",
"class-variance-authority": "^0.7.0",
"clsx": "^2.0.0",
"tailwind-merge": "^1.14.0"
},
"devDependencies": {
"@types/react": "^18.2.43",
"@types/react-dom": "^18.2.17",
"@vitejs/plugin-react": "^4.2.1",
"typescript": "^5.2.2",
"vite": "^5.0.0",
"tailwindcss": "^3.0.0-alpha.25",
"eslint-vite": "^0.0.0-alpha.25"
}
}